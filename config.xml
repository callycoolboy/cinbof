<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.cibof.app" version="1.0.0" android-versionCode="10000" ios-CFBundleVersion="1.0.0">
  <name>CINBOF Mobile Banking</name>
    <description>
        No 1 Online Banking.
    </description>
    <author email="info@cibof.com" href="">
        CINBOF Mobile Banking
    </author>
  <content src="index.html"/>


<!--<preference name='pgb-builder-version' value='2' />
  <preference name="android-build-tool" value="gradle" />-->
<preference name="android-minSdkVersion" value="15" />
  <preference name="android-targetSdkVersion" value="28"/> //android 4.2
<preference name="loadUrlTimeoutValue" value="60000" />
    <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  
  <platform name="android">
    <allow-intent href="market:*"/>
    <!-- ONESIGNAL-->
    <icon src="res/icon/android/mipmap-ldpi.png" density="ldpi"/>
    <icon src="res/icon/android/mipmap-mdpi.png" density="mdpi"/>
    <icon src="res/icon/android/mipmap-hdpi.png" density="hdpi"/>
    <icon src="res/icon/android/mipmap-xhdpi.png" density="xhdpi"/>
    <icon src="res/icon/android/mipmap-xxhdpi.png" density="xxhdpi"/>
    <icon src="res/icon/android/mipmap-xxxhdpi.png" density="xxxhdpi"/>
    

      </platform>
  
  <platform name="windows">
    <icon src="res/icon/android/mipmap-xxxhdpi.png" platform="windows" width="310" height="150"/>
    <icon src="res/icon/android/mipmap-xxxhdpi.png" platform="windows" width="744" height="360"/>
    </platform>
  
</widget>
